<UserControl x:Class="Beijing_Inn_Order_System.Screens.OrderPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             xmlns:Behaviors="clr-namespace:Beijing_Inn_Order_System"
             xmlns:SizeButton="clr-namespace:Beijing_Inn_Order_System.Items"
             xmlns:CustomControls="clr-namespace:Beijing_Inn_Order_System.CustomControls"
             xmlns:uc="clr-namespace:Beijing_Inn_Order_System"
             mc:Ignorable="d" Loaded="OnLoad" d:DesignWidth="1024" d:DesignHeight="758"
             xmlns:pages="clr-namespace:Beijing_Inn_Order_System.Screens.OrderPageElements"
             DataContext="{Binding RelativeSource={RelativeSource Self}}" Unloaded="UserControl_Unloaded">

    <Grid>

        <TabControl>
            <TabItem Header="PICK ITEMS" x:Name="PickItemsTab"></TabItem>
            <TabItem Header="DELIVERY" x:Name="DeliveryTab"></TabItem>
            <TabItem Header="PRINT" x:Name="PrintTab"></TabItem>
        </TabControl>
    </Grid>
</UserControl>
